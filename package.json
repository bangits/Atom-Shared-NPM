{
  "name": "@atom/common",
  "version": "3.2.7",
  "main": "",
  "types": "./dist/atom-common.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "concurrently \"yarn panda codegen\" \"tsc && vite build --watch\" \"yarn vite preview\"",
    "build": "yarn panda codegen && tsc && vite build",
    "cd": "npm run build && semantic-release",
    "ci": "npm i",
    "commit": "git add . && git-cz && git push",
    "build-declarations": "tsc -p tsconfig.declarations.json && tscpaths -p tsconfig.declarations.json -s ./src -o ./dist",
    "lint": "eslint src --ext js,ts,tsx",
    "format": "yarn pretty-quick",
    "check-format": "prettier --check .",
    "prepare": "husky install",
    "start-app": "docker-compose up --build",
    "kill-app": "docker-compose down",
    "pre-commit": "yarn format && yarn lint",
    "pre-push": "yarn format"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/redux-logger": "^3.0.9",
    "@types/systemjs": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-ts-react-important-stuff": "^3.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "inversify": "^5.1.1",
    "postcss": "^8.4.31",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "redux-logger": "^3.0.6",
    "reflect-metadata": "^0.1.13",
    "single-spa": "^5.9.3",
    "single-spa-react": "^4.6.0",
    "tsconfig-paths": "^3.11.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.3.4",
    "vite": "^4.4.10"
  },
  "dependencies": {
    "@atom/authorization": "^0.1.7",
    "@atom/cms-management": "^0.3.0",
    "@atom/design-system": "^5.6.35",
    "@automapper/classes": "^7.0.2",
    "@automapper/core": "^7.0.2",
    "@hookform/resolvers": "3.3.0",
    "@microsoft/signalr": "^6.0.1",
    "@microsoft/signalr-protocol-msgpack": "^6.0.1",
    "@pandacss/dev": "^0.15.5",
    "@vitejs/plugin-react": "^4.1.0",
    "axios": "^0.23.0",
    "dayjs": "^1.10.7",
    "formik": "^2.2.9",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-hook-form": "^7.47.0"
  }
}
